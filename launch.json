{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Flask",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/app.py",
        "args": ["--no-reload"],
        "env": {
            "FLASK_APP": "app.py",
            "FLASK_ENV": "development",
            "MYSQL_UNIX_SOCKET": "/Applications/XAMPP/xamppfiles/var/mysql/mysql.sock"
        },
        "jinja": true
    }
    ]
  }
  